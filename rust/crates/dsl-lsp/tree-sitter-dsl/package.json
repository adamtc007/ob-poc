{
  "name": "tree-sitter-dsl",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Onboarding DSL",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "tree-sitter",
    "dsl",
    "lisp"
  ],
  "author": "OB-POC Team",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.26.3"
  },
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.dsl",
      "injection-regex": "dsl",
      "file-types": [
        "dsl",
        "obl",
        "onboard"
      ],
      "highlights": "queries/highlights.scm"
    }
  ]
}
