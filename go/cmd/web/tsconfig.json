{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ES2020",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./static",
    "rootDir": "./static",
    "sourceMap": true
  },
  "include": ["static/**/*.ts"],
  "exclude": ["node_modules"]
}
