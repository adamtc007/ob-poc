{
  "file_types": {
    "Clojure": ["dsl", "obdsl"],
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "cargo": {
          "features": ["database"],
          "buildScripts": {
            "enable": true,
          },
        },
        "check": {
          "command": "clippy",
          "features": ["database"],
        },
        "procMacro": {
          "enable": true,
        },
      },
    },
    "dsl-lsp": {
      "binary": {
        "path": "/Users/adamtc007/Developer/ob-poc/rust/target/release/dsl-lsp",
        "env": {
          "DSL_CONFIG_DIR": "/Users/adamtc007/Developer/ob-poc/rust/config",
        },
      },
    },
  },
  "languages": {
    "Clojure": {
      "language_servers": ["dsl-lsp", "!clojure-lsp"],
    },
  },
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o",
    },
    "version": "2",
  },
  "features": {
    "copilot": true,
  },
  "show_copilot_suggestions": true,
  "copilot": {
    "disabled_globs": [],
    "enable_completions": true,
    "enable_chat": true,
  },
  "language_models": {
    "copilot_chat": {
      "available_models": [
        {
          "name": "gpt-4o",
          "display_name": "GPT-4o",
          "provider": "copilot_chat",
          "max_tokens": 128000,
        },
        {
          "name": "gpt-4",
          "display_name": "GPT-4",
          "provider": "copilot_chat",
          "max_tokens": 8192,
        },
        {
          "name": "gpt-3.5-turbo",
          "display_name": "GPT-3.5 Turbo",
          "provider": "copilot_chat",
          "max_tokens": 4096,
        },
      ],
    },
  },
  "inline_completions": {
    "provider": "copilot",
  },
}
