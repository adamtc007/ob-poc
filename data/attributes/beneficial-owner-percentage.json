  "semantic": {
    "description": "The percentage of ownership or control that a natural person exercises over the legal entity, directly or indirectly. Used to determine if the person meets the threshold for beneficial ownership reporting under FinCEN regulations.",
    "context": "Required for CIP/KYC compliance when onboarding corporate entities. Threshold is typically 25% for US regulations, 10% for EU entities.",
    "related_concepts": [
      "ultimate-beneficial-owner",
      "control-person",
      "ownership-structure",
      "fincen-rule"
    ],
    "usage_examples": [
      "Natural person owns 30% of shares directly",
      "Person controls 15% through holding company + 12% directly = 27% total"
    ],
    "regulatory_citations": [
      "31 CFR 1010.230",
      "FinCEN CDD Rule"
    ]
  },
